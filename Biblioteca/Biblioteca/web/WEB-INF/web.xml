    <servlet-mapping>
        <servlet-name>AdicionarFuncionarioServlet</servlet-name>
        <url-pattern>/AdicionarFuncionario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RealizarEntregaServlet</servlet-name>
        <url-pattern>/RealizarEntrega</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TesteServlet</servlet-name>
        <url-pattern>/Teste</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>30</session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <listener>
        <listener-class>
            org.apache.commons.fileupload.servlet.FileCleanerCleanup
        </listener-class>
    </listener>
</web-app>
