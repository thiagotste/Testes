        <servlet-class>Control.AdicionarClienteServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControlarClienteServlet</servlet-name>
        <servlet-class>Control.ControlarClienteServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdicionarLivroServlet</servlet-name>
        <servlet-class>Control.AdicionarLivroServlet</servlet-class>
    </servlet>
    <servlet>
