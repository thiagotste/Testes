    </servlet>
    <servlet>
        <servlet-name>AdicionarEscritorServlet</servlet-name>
        <servlet-class>Control.AdicionarEscritorServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdicionarTipoServlet</servlet-name>
        <servlet-class>Control.AdicionarTipoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ItemPainelServlet</servlet-name>
        <servlet-class>Control.ItemPainelServlet</servlet-class>
    </servlet>
