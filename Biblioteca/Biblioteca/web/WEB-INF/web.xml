    </servlet>
    <servlet>
        <servlet-name>DownloadServlet</servlet-name>
        <servlet-class>Control.DownloadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdicionarFuncionarioServlet</servlet-name>
        <servlet-class>Control.AdicionarFuncionarioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RealizarEntregaServlet</servlet-name>
        <servlet-class>Control.RealizarEntregaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TesteServlet</servlet-name>
        <servlet-class>Control.TesteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdicionarClienteServlet</servlet-name>
        <url-pattern>/AdicionarCliente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControlarClienteServlet</servlet-name>
        <url-pattern>/ControlarCliente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdicionarLivroServlet</servlet-name>
        <url-pattern>/AdicionarLivro</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdicionarBibliotecaServlet</servlet-name>
        <url-pattern>/AdicionarBiblioteca</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdicionarEscritorServlet</servlet-name>
        <url-pattern>/AdicionarEscritor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdicionarTipoServlet</servlet-name>
        <url-pattern>/AdicionarTipo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ItemPainelServlet</servlet-name>
        <url-pattern>/ItemPainel</url-pattern>
